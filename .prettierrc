{
    "useTabs": true,
    "singleQuote": false,
    "semi": true,
    "trailingComma": "none",
    "printWidth": 100,
    "endOfLine": "lf",
    "plugins": [
        "prettier-plugin-svelte",
        "prettier-plugin-tailwindcss"
    ],
    "tailwindAttributes": [
        "inner-class",
        "input-class",
        "textarea-class",
        "bg-class",
        "icon-class",
        "track-x-class",
        "track-y-class",
        "thumb-x-class",
        "thumb-y-class",
        "heading-class",
        "button-class",
        "content-class",
        "error-class"
    ],
    "tailwindFunctions": [
        "twMerge",
        "tv"
    ],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ]
}